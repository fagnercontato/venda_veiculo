generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model venda_veiculo {
  id                  String   @id @default(auto()) @map("_id") @db.ObjectId
  cpf                 String
  cpfComprador        String
  dataAtualizacao     DateTime? @default(now()) @map("updated_v")
  dataProcessamento   DateTime? @db.Date
  dataVenda           DateTime? @default(now()) @map("created_v") @db.Date
  idTAbelaVeiculo     String
  pagamentoProcessado Boolean @default(false)
  valorVenda          Float
}
